= t(".title_editing_admin_content")

= form_for [:admin, @admin_content] do |f|
  = render(:partial => "/admin/contents/templates/error_form", :locals => {:admin_content => @admin_content})

  %div.actions
    %fieldset
      = render(:partial => "/admin/contents/form", :locals => {:f => f, :admin_content => @admin_content, :admin_panel => @admin_panel, :admin_categoryall => @admin_categoryall, :admin_product => @admin_product})
      %p
        = f.submit(t("admin.panels.edit.update_submit"), :name => "update_updcontents_#{@admin_panel.id}")  
